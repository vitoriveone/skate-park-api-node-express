<table class="table w-50 m-auto">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Foto</th>
            <th scope="col">Nombre</th>
            <th scope="col">AÃ±os de experiencia</th>
            <th scope="col">Especialidad</th>
            <th scope="col">Estado</th>
          </tr>
        </thead>
        <tbody>
          {{#each skaters}}
    <tr>
      <th scope="row">{{incrementIndex @index}}</th>
      <td><img src="{{url}}/img/avatar/{{this.foto}}" alt="" class="rounded avatar"></td>
      <td>{{this.nombre}}</td>
